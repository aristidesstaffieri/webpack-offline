    "React",
    "Babel",
		"Service Workers",
		"Offline"
  ],
  "author": "Aristides Staffieri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aristidesstaffieri/webpack-offline.git/issues"
  },
  "homepage": "https://github.com/aristidesstaffieri/webpack-offline.git#readme",
  "dependencies": {
    "react": "^0.14.6",
    "react": "^0.14.0",
    "react-addons-pure-render-mixin": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
		"babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "chai": "^3.3.0",
    "css-loader": "^0.21.0",
    "jsdom": "^7.0.1",
    "mocha": "^2.3.3",
    "react-addons-test-utils": "^0.14.2",
    "react-hot-loader": "^1.3.0",
    "standard": "^5.3.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
	"standard": {
    "globals": [
      "it",
      "describe"
    ]
  }
